@model ClientDB.Models.ClientRegistrationPAModel





<form>

    <script type="text/javascript">
        $(document).ready(function () {
            function CheckboxApprovdVisitor() {
                if ($('#IsOnCampusWithStaff').prop("checked") == true || $('#IsOnCampusAlone').prop("checked") == true || $('#IsOffCampus').prop("checked") == true) {
                    $('#ApprovedVisitor').prop("checked", true);
                }
                else {
                    $('#ApprovedVisitor').prop("checked", false);
                }
            }
        });
    </script>
    <div id="EmerQuick">
        <table style="width: 100%">

            <tr>
                <td colspan="4">
                    <div>
                        <h4 style="font-weight:100">Approved Visitors Quick View</h4>
                    </div>
                </td>
            </tr>
            <tr>
                @foreach (var item in Model.ApprovedVisitorIndi)
                {

                    <td>
                        @*<h2>Contact Name: @item.FirstName , @item.LastName</h2>*@
                        <h2>Contact Name: @item.FUllName</h2>
                        <b>Relationship:</b> @Model.RelationNameVisit<br />
                        @if (@Model.SpouseName != null)
                        {
                            <b>Spouse:</b> @Model.SpouseName<br />
                        }
                        else
                        {
                            <b>Spouse: </b><br />
                        }

                        <b>Contact Notes:</b> @Model.Note
                    </td>
                }
            </tr>



            <tr>
                <td colspan="4">

                    <table id="tblContacList" style="width: 100%">
                        <tr class="HeaderStyle">
                            <th class="tdLabel">Contact Type</th>
                            <th class="tdLabel">Phone Number</th>
                            <th class="tdLabel">Address</th>
                            <th class="tdLabel">Email</th>
                        </tr>
                        @foreach (var item in Model.ApprovedVisitorIndi)
                        {


                            <tr class="RowStyle">
                                <td>Home</td>
                                <td>
                                    Home:@item.HomePhone<br />
                                    Mobile:@item.HomeMobilePhone<br />
                                    Other:@item.HomeWorkPhone Ext:@item.HomeExtension
                                </td>
                                <td>
                                    Street:@item.HomeAddressLine2<br />
                                    Floor/Suite#:@item.HomeAddressLine1<br />
                                    City:@item.HomeCity<br />
                                    State:@item.HmeState<br />
                                    Zip Code:@item.HomeZip

                                </td>
                                <td>
                                    Home:@item.HomeEmail<br />
                                    Other:@item.HomeOtherMail
                                </td>

                            </tr>

                        }
                        @foreach (var item in Model.ApprovedVisitorIndi)
                        {
                            <tr class="RowStyle">
                                <td>Work</td>



                                <td>
                                    Primary Contact:@item.WorkHomePhone Ext:@item.WorkExtension<br />
                                    Mobile:@item.WorkMobilePhone<br />
                                    Other:@item.WorkPhone Ext:@item.OtherExtension2
                                </td>
                                <td>
                                    Occupation:@item.Occupation<br />
                                    Organization:@item.Employer<br />
                                    Street:@item.WorkAddressLine2<br />
                                    Floor/Suite#:@item.WorkAddressLine1<br />
                                    City:@item.WorkCity<br />
                                    State:@item.WrkState<br />
                                    Zip Code:@item.WorkZip
                                </td>
                                <td>
                                    Work:@item.WorkEmail
                                </td>
                            </tr>
                        }

                        @foreach (var item in Model.ApprovedVisitorIndi)
                        {
                            <tr class="RowStyle">
                                <td>Other</td>



                                <td>
                                    Primary Contact:@item.OtherHomePhone Ext:@item.OtherExtension<br />
                                    Mobile:@item.OtherMobilePhone<br />
                                    Other:@item.OtherWorkPhone Ext:@item.OtherExtension3
                                </td>
                                <td>
                                    Street:@item.OtherAddressLine2<br />
                                    Floor/Suite#:@item.OtherAddressLine1<br />
                                    City:@item.OtherCity<br />
                                    State:@item.OthState<br />
                                    Zip Code:@item.OtherZip
                                </td>
                                <td></td>
                            </tr>
                        }
                    </table>
                </td>
            </tr>

            <tr>
                <td colspan="4">
                    
                </td>
            </tr>

            <tr>

                @if (Model.IsGuardian == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Guardian</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Guardian</td>
                }

                @if (Model.IsCustody == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Custody</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Custody</td>
                }

                @if (Model.IsNextOfKin == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Next Of Kin</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Next Of Kin</td>
                }
               @* @if (Model.IsApprovedVisitor == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Approved Visitor</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Approved Visitor</td>
                }*@
                @if (Model.IsSchool == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />School Contact</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />School Contact</td>
                }
            </tr>

            <tr>
                @if (Model.IsEmergency == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Emergency Contact</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Emergency Contact</td>
                }
                @if (Model.Isincident == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Incident Contact</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Incident Contact</td>
                }

                @if (Model.IsCorrespondence == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Correspondence Contact</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Correspondence Contact</td>
                }

                @if (Model.IsBilling == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Billing Contact</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Billing Contact</td>
                }
            </tr>
            
            <tr>
                
            </tr>
           
            @*<tr>
                
                @if (Model.IsOnCampusWithStaff == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />On Campus, With Staff</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />On Campus, With Staff</td>
                }
            
                @if (Model.IsOnCampusAlone == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />On Campus, Alone</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />On Campus, Alone</td>
                }
            
                @if (Model.IsOffCampus == true)
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" checked="checked" />Off Campus</td>
                }
                else
                {
                    <td style="text-align:left"><input style="width:auto;" type="checkbox" onclick="return false" />Off Campus</td>
                }
            </tr>*@
        </table>
        <tr>

            <td>
                <fieldset>
                    <legend style="font-size:12px;"><b>Approved Visitor</b></legend>
                    <label class="lblSpan">On Campus, With Staff</label><span class="nospan-align">*</span>
                    <label class="lblSpan" style="padding-left: 100px">On Campus, Alone</label><span class="nospan-align">*</span>
                    <label class="lblSpan" style="padding-left: 90px">Off Campus</label><span class="nospan-align">*</span><br />

                    @Html.CheckBoxFor(x => x.IsOnCampusWithStaff, new { onclick = "", disabled = "disabled" })
                    @Html.CheckBoxFor(x => x.IsOnCampusAlone, new { @class = "", @style = "margin-left: 204px;", onclick = "", disabled = "disabled" })
                    @Html.CheckBoxFor(x => x.IsOffCampus, new { @class = "", @style = "margin-left: 168px;", onclick = "", disabled = "disabled" })




                </fieldset>
            </td>
        </tr>

    </div>
</form>