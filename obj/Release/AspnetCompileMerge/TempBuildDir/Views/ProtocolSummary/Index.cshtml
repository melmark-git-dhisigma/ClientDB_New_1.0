    $(document).ready(function () {
        
        function addRow() {
            var html = '<tr style="text-align:center">' +
                '<td><textarea name="ATList[' + ATCounter + '].type" style="width:100%"></textarea></td>' +
                '<td><textarea name="ATList[' + ATCounter + '].schedule" style="width:100%"></textarea></td>' +
                '<td><textarea name="ATList[' + ATCounter + '].storage" style="width:100%"></textarea></td>' +
                '<td style="width:50px"><input type="button" value="Delete" class="btnDel" style="float:right" onclick="DeleteRow(this);" /></td>' +
                '</tr>'
            $(html).appendTo($("#table2 > tbody"));
            ATCounter++;
        };
        $(".btnAdd1").click(addRow);
    });

    function DeleteRow(btnDel) {
        if (typeof (btnDel) == "object") {
            $(btnDel).closest("tr").remove();
        } else {
            return false;
        }
    }

    $(document).ready(function () {
        function addRow() {
